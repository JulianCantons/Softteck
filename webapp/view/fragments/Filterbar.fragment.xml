<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:fb="sap.ui.comp.filterbar">

    <fb:FilterBar id="filterbar" persistencyKey="myPersKey" 
    useToolbar="false" search=".onSearch" filterChange=".onFilterChange" 
    showGoOnFB="true" showFilterConfiguration="false" 
    reset="onRestoreFilterFields">
        <fb:filterGroupItems>
            <fb:FilterGroupItem id="_IDGenFilterGroupItem1" name="IdAdmin" label="Id Admin" groupName="Group1" visibleInFilterBar="true">
                <fb:control>
                    <Input id="idAdminInput" name="IdAdmin" showValueHelp="true" change="onChangeAdminId" valueHelpRequest="onValueHelpAdmin" busyIndicatorDelay="0" />
                </fb:control>
            </fb:FilterGroupItem>
            <fb:FilterGroupItem id="_IDGenFilterGroupItem2" name="Nombre" label="Nombre" groupName="Group1" visibleInFilterBar="true">
                <fb:control>
                    <Input id="NombreInput" name="Nombre" showValueHelp="true" change="onChangeNombre" valueHelpRequest="onValueHelpNombre" busyIndicatorDelay="0" />
                </fb:control>
            </fb:FilterGroupItem>
        </fb:filterGroupItems>
    </fb:FilterBar>

</core:FragmentDefinition>

